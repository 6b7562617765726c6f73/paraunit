{
  "alias": "paraunit.phar",
  "chmod": "0755",
  "compression": "GZ",
  "directories": [
    "src"
  ],
  "files": [
    "LICENSE",
    "src/Paraunit/Bin/paraunit",
    "vendor/phpunit/phpunit/phpunit"
  ],
  "finder": [
    {
      "name": "*.php",
      "in": "vendor",
      "exclude": [
        "tests",
        "Tests",
        "spec",
        "fixtures"
      ]
    }
  ],
  "git-version": "dev-master",
  "main": "src/Paraunit/Bin/phar",
  "shebang": "#!/usr/bin/env php",
  "output": "paraunit.phar",
  "stub": true
}
